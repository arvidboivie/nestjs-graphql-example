    "graphql-tools": "^4.0.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.2"
  },
  "devDependencies": {
    "copyfiles": "^2.1.0",
    "nodemon": "^1.19.1",
    "tscpaths": "^0.0.9"
